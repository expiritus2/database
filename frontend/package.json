{
  "name": "database",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8080 react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:ui": "mocha --experimental-modules --parallel --retries 1 --reporter mochawesome --require mochawesome/register --config ./.mocharc.json ./src/tests/ui/test/**/*.test.mjs",
    "test:ui:single": "mocha --experimental-modules --config ./.mocharc.json ./src/tests/ui/test/**/*.test.mjs",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js src",
    "commit": "./node_modules/cz-customizable/standalone.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "formik": "^2.2.6",
    "lodash-es": "^4.17.20",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-flatpickr": "^3.10.6",
    "react-hint": "^3.2.0",
    "react-number-format": "^4.4.4",
    "react-phone-input-2": "^2.13.9",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-router-prop-types": "^1.0.5",
    "react-scripts": "4.0.1",
    "react-select-search": "^2.2.4",
    "react-slider": "^1.1.4",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sass": "^1.30.0",
    "toastr": "^2.1.4",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.9",
    "yup-password": "^0.1.2",
    "yup-phone": "^1.2.19"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.3.1",
    "husky": "^4.3.5",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "redux-devtools-extension": "^2.13.8"
  }
}
